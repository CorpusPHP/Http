{
	"name": "corpus/http",
	"description": "HTTP Status Codes and Exceptions",
	"type": "library",
	"license": "MIT",
	"authors": [
		{
			"name": "Jesse Donat",
			"email": "donatj@gmail.com"
		}
	],
	"require": {
		"php": "^7.4 || ^8.0"
	},
	"autoload": {
		"psr-4": {
			"Corpus\\Http\\": "src/"
		}
	},
	"require-dev": {
		"corpus/coding-standard": "^0.10.0",
		"friendsofphp/php-cs-fixer": "^3.4",
		"phpstan/phpstan": "^1.12",
		"phpunit/phpunit": "^9.6",
		"squizlabs/php_codesniffer": "^4.0"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
